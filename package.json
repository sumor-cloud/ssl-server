{
  "name": "@sumor/ssl-server",
  "description": "An SSL web server for Expressjs support HTTP/2",
  "version": "1.0.2",
  "license": "MIT",
  "repository": "sumor-cloud/ssl-server",
  "type": "module",
  "dependencies": {
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "selfsigned": "^2.4.1",
    "spdy": "^4.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2"
  },
  "files": [
    "src",
    "LICENSE",
    "index.js"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
