    "publish-release-patch": "npm version patch && git push --tags",
    "publish-release-minor": "npm version minor && git push --tags",
    "publish-release-major": "npm version major && git push --tags",
    "prepare": "husky",
    "check": "eslint --fix . && npm audit fix && npm test"
